<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--个人主页动态模块(统计类型)配置-->
    <!--模块背景图-->
    <drawable name="starapp_life_home_dynamic_fragment_statis_bg">#ffffff</drawable>
    <!--分割线-顶部-->
    <drawable name="starapp_life_home_dynamic_fragment_statis_line_top">@color/starapp_life_color_8</drawable>
    <!--分割线-中间-->
    <drawable name="starapp_life_home_dynamic_fragment_statis_line">@color/starapp_life_color_8</drawable>
    <!--分割线-底部-->
    <drawable name="starapp_life_home_dynamic_fragment_statis_line_bottom">@color/starapp_life_color_8</drawable>
    <!--1.左边图片最小宽度-->
    <dimen name="starapp_life_home_dynamic_fragment_statis_left_img_min_width">18dp</dimen>
    <!--主体部分最小高度-->
    <dimen name="starapp_life_home_dynamic_fragment_statis_body_min_height">44dp</dimen>
    <!--主体部分内容边距-->
    <dimen name="starapp_life_home_dynamic_fragment_statis_body_padding_left">0dp</dimen>
    <dimen name="starapp_life_home_dynamic_fragment_statis_body_padding_top">16dp</dimen>
    <dimen name="starapp_life_home_dynamic_fragment_statis_body_padding_right">10dp</dimen>
    <dimen name="starapp_life_home_dynamic_fragment_statis_body_padding_bottom">16dp</dimen>
    <!--2.标题文字大小,颜色,左边距-->
    <dimen name="starapp_life_home_dynamic_fragment_statis_title_size">@dimen/starapp_life_text_13</dimen>
    <color name="starapp_life_home_dynamic_fragment_statis_title_color">@color/starapp_life_color_2</color>
    <dimen name="starapp_life_home_dynamic_fragment_statis_title_margin_left">0dp</dimen>
    <!--3.统计结果文字大小,颜色,左边距,最大宽度-->
    <dimen name="starapp_life_home_dynamic_fragment_statis_reward_size">@dimen/starapp_life_text_13</dimen>
    <color name="starapp_life_home_dynamic_fragment_statis_reward_color">@color/starapp_life_color_2</color>
    <dimen name="starapp_life_home_dynamic_fragment_statis_reward_margin_left">8dp</dimen>
    <dimen name="starapp_life_home_dynamic_fragment_statis_reward_max_width">200dp</dimen>
</resources>