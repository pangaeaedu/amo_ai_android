<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.




    -->
    <style name="maincomponent_AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.




        -->
    </style>

    <!-- Application theme. -->
    <style name="maincomponent_AppTheme" parent="maincomponent_AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="maincomponent_main_foot_tab_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/skin_maincomponent_10sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/maincom_foot_text_color</item>
    </style>

    <style name="maincomponent_jay_main_foot_count">
        <item name="android:textSize">@dimen/skin_maincomponent_10sp</item>
        <item name="android:textColor">@color/skin_maincomponent_white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    <!-- 设置页面 与个人信息左侧  字体 -->
    <style name="maincomponent_setting_txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- 设置页面   图片 -->
    <style name="maincomponent_setting_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>
    <!-- 设置页面布局 -->
    <style name="maincomponent_setting_layout_item">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">10dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/skin_maincomponent_setting_item_bg</item>
    </style>
    <!-- 横向分割符 -->
    <style name="maincomponent_task_separateline_horizontal">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:background">@drawable/skin_maincomponent_setting_separateline_horizontal</item>
    </style>
    <!-- 半透明主题 -->
    <style name="Transparent">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:windowBackground">@color/skin_maincomponent_theme_transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="UpdateDialogStyle">
        <item name="android:windowBackground">@color/skin_maincomponent_half_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>　　　　　　　
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="AppfactoryDevToolStyle">
        <item name="android:layout_marginTop">@dimen/skin_maincomponent_dp10</item>
        <item name="android:layout_marginEnd">@dimen/skin_maincomponent_dp10</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>

    <style name="MainComDevToolStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/skin_maincomponent_dp40</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/skin_maincomponent_20sp</item>
        <item name="android:layout_marginStart">@dimen/skin_maincomponent_dp10</item>

    </style>

    <style name="devtool_UIMonitor_config">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="devtool_UIMonitor">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="resign_check_DialogStyle">
        <item name="android:windowBackground">@color/resign_check_dialog_half_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>　　　　　　　
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
	<!--整个webview界面的主题-->
	<style name="ApfWebView">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>
     <!--webview界面页面查找中三个按钮的主题-->
    <style name="webcomponent.Feature.FindInPage.Buttons" parent="">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
        <item name="tint">#ff000000</item>
    </style>

    <style name="maincomponent_maintenance_advance_notice_dialog_style">
        <item name="android:windowBackground">@color/skin_maincomponent_half_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>　　　　　　　
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
	
	
	
    <style name="Theme.AppCompat.Translucent.Screen" parent="Theme.AppCompat">
        <item name="android:windowBackground">@color/webcomponent_screen_pre_bg_color</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/ScreenAnimation</item>
    </style>

    <style name="ScreenAnimation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@null</item>
    </style>
	
	<!-- 应用升级相关资源 begin -->
	<!--  升级对话框总布局  -->
    <style name="new_common_update_dialog_root_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/new_common_update_update_dialog_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
    </style>

    <!--  升级对话框文字标题  -->
    <style name="new_common_update_dialog_txt_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:layout_marginBottom">12dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/new_common_update_update_dialog_text</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!--  升级对话框图标  -->
    <style name="new_common_update_dialog_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:visibility">gone</item>
    </style>

    <!--  升级对话框文字内容  -->
    <style name="new_common_update_dialog_txt_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingExtra">7dp</item>
        <item name="android:maxHeight">100dp</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:textColor">@color/new_common_update_update_dialog_text</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!--  升级对话框分割线，当下载失败、下载消耗流量等场景时可见  -->
    <style name="new_common_update_dialog_tip_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:layout_marginBottom">15dp</item>
        <item name="android:background">@color/new_common_update_dialog_divider_line</item>
        <item name="android:visibility">gone</item>
    </style>

    <!--  升级对话框流量消耗文字提示  -->
    <style name="new_common_update_dialog_txt_network_tip">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingExtra">7dp</item>
        <item name="android:textColor">@color/new_common_update_dialog_tip</item>
        <item name="android:textSize">12sp</item>
    </style>

    <!--  升级对话框流量下载失败文字提示  -->
    <style name="new_common_update_dialog_txt_download_failed_tip">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">7dp</item>
        <item name="android:lineSpacingExtra">7dp</item>
        <item name="android:textColor">@color/new_common_update_dialog_fail_tip</item>
        <item name="android:textSize">12sp</item>
    </style>

    <!--  升级对话框流量下载中文字提示  -->
    <style name="new_common_update_dialog_txt_downloading_tip">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">7dp</item>
        <item name="android:lineSpacingExtra">7dp</item>
        <item name="android:textColor">@color/new_common_update_dialog_tip</item>
        <item name="android:textSize">12sp</item>
    </style>

    <!--  升级对话框进度条父布局  -->
    <style name="new_common_update_dialog_progress_root_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:visibility">visible</item>
    </style>

    <!--  升级对话框进度条布局  -->
    <style name="new_common_update_dialog_progress_layout">
        <item name="android:layout_width">195dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <!--  升级对话框进度条  -->
    <style name="new_common_update_dialog_progress" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:layout_width">190dp</item>
        <item name="android:layout_height">19dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:max">100</item>
        <item name="android:progress">0</item>
        <item name="android:thumbOffset">2dp</item>
    </style>

    <!--  升级对话框进度条文字  -->
    <style name="new_common_update_dialog_txt_progress">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:textColor">@color/new_common_update_dialog_content</item>
        <item name="android:textSize">19sp</item>
    </style>

    <!--  升级对话框按钮父布局  -->
    <style name="new_common_update_dialog_btn_root_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>

    <!--  升级对话框非强制更新按钮布局  -->
    <style name="new_common_update_dialog_btn_unforce_update_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:visibility">gone</item>
    </style>

    <!--  升级对话框强制更新按钮布局  -->
    <style name="new_common_update_dialog_btn_force_update_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!--  升级对话框取消按钮  -->
    <style name="new_common_update_dialog_btn_cancel" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:width">80dp</item>
        <item name="android:height">36dp</item>
        <item name="android:background">@drawable/new_common_update_download_dialog_btn_selector_selector</item>
        <item name="android:textColor">@color/new_common_update_dialog_content</item>
        <item name="android:textSize">17sp</item>
    </style>

    <!--  升级对话框确认按钮  -->
    <style name="new_common_update_dialog_btn_confirm" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:width">80dp</item>
        <item name="android:height">36dp</item>
        <item name="android:background">@drawable/new_common_update_download_dialog_btn_selector_selector</item>
        <item name="android:textColor">@color/new_common_update_dialog_content</item>
        <item name="android:textSize">17sp</item>
    </style>
	<!-- 应用升级相关资源 end -->

</resources>