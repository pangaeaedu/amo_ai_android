<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--持续签到奖励模式的签到页面-->
    <!--背景图-->
    <drawable name="starapp_life_sign_star_background">#f5f5f5</drawable>
    <!--日历模块和info list模块之间分割线颜色-->
    <color name="starapp_life_sign_star_info_top_line_color">@color/starapp_life_color_8</color>
    <!--1.info list背景图-->
    <drawable name="starapp_life_sign_star_info_list_background">#ffffff</drawable>
    <!--1.info list内部分割线颜色-->
    <color name="starapp_life_sign_star_info_line_color">@color/starapp_life_color_8</color>
    <!--1.info list文字颜色-->
    <color name="starapp_life_sign_star_info_text_color">@color/starapp_life_color_2</color>
    <!--2.累计签到奖励背景图-->
    <drawable name="starapp_life_sign_star_fragment_background">#ffffff</drawable>
    <!--2.累计签到奖励标题颜色-->
    <color name="starapp_life_sign_star_fragment_title_color">@color/starapp_life_color_1</color>
    <!--2.累计签到奖励:任务状态文字颜色(已完成)-->
    <color name="starapp_life_sign_star_fragment_status_text_color_complete">@color/starapp_life_color_17</color>
    <!--2.累计签到奖励:任务状态文字颜色(进行中)-->
    <color name="starapp_life_sign_star_fragment_status_text_color_ongoing">@color/starapp_life_color_2</color>
    <!--2.累计签到奖励:任务状态文字颜色(未开始)-->
    <color name="starapp_life_sign_star_fragment_status_text_color_notsart">@color/starapp_life_color_5</color>
</resources>